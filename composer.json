{
    "name": "kurbar/wildduck-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Karl Viiburg",
            "email": "karl.viiburg@twn.ee"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Wildduck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wildduck\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "laravel/framework": "^5.6",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wildduck\\ServiceProvider"
            ],
            "aliases": {
                "Wildduck": "Wildduck\\Facade"
            }
        }
    }
}
